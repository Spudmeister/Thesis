@article{magnet,
title = {Precise mapping of the magnetic field in the CMS barrel yoke using cosmic rays},
author = {Chatrchyan, S. and et al.,},
abstractNote = {The CMS detector is designed around a large 4 T superconducting solenoid, enclosed in a 12000-tonne steel return yoke. A detailed map of the magnetic field is required for the accurate simulation and reconstruction of physics events in the CMS detector, not only in the inner tracking region inside the solenoid but also in the large and complex structure of the steel yoke, which is instrumented with muon chambers. Using a large sample of cosmic muon events collected by CMS in 2008, the field in the steel of the barrel yoke has been determined with a precision of 3 to 8% depending on the location.},
doi = {10.1088/1748-0221/5/03/T03021},
journal = {Journal of Instrumentation},
issn = {1748-0221},
number = 03,
volume = 5,
place = {United States},
year = {2010},
month = {3}
}
