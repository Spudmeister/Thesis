@article{osti_6892658,
title = {Electroweak baryogenesis in left-right-symmetric models},
author = {Mohapatra, R.N. and Zhang, X.},
abstractNote = {We explore the possibility of electroweak baryogenesis in left-right-symmetric models with a low scale for the local [ital B][minus][ital L] symmetry breaking. We find that the minimal model cannot generate enough [ital n][sub [ital B]]/[ital s]; one way to rectify the situation is to add a parity-odd real Higgs-singlet field to the model. In this case, we obtain a lower limit on the [ital W][sub [ital L]-][ital W[ital R]] mixing parameter [zeta][ge]0.2([ital M][sub [ital W][ital L]]/[ital M][sub [ital W][ital R]])[sup 2]. We study its phenomenological implications.},
